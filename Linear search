function search(arr, x) {
    const n = arr.length;
    
    // Iterate over the array in order to
    // find the key x
    for (let i = 0; i < n; i++)
        if (arr[i] == x)
            return i;
    return -1;
}
    // Driver Code

    let arr = [ 2, 3, 4, 10, 40 ];
    let x = 10;

    let result = search(arr, x);
    (result == -1)
        ? console.log("Element is not present in array")
        : console.log("Element is present at index " + result);


output:
Present at Index 3
